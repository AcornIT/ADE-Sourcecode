   {1}._order           =    {2}._order          
   {1}._icon_up         =    {2}._icon_up        
   {1}._icon_up_x       =    {2}._icon_up_x      
   {1}._icon_up_y       =    {2}._icon_up_y      
   {1}._h_up_image      =    {2}._h_up_image     
   {1}._icon_down       =    {2}._icon_down      
   {1}._icon_down_x     =    {2}._icon_down_x    
   {1}._icon_down_y     =    {2}._icon_down_y    
   {1}._New_Template    =    {2}._New_Template   
   {1}._dbconnect       =    {2}._dbconnect      
   {1}._attr            =    {2}._attr           
   {1}._files           =    {2}._files          
.
