  POSITION 4
  INITIAL "0"
  INITIAL-SA ?
  HELP "Data type"
  HELP-SA ""
  LABEL "Data Type"
  LABEL-SA ?
  COLUMN-LABEL ?
  COLUMN-LABEL-SA ?
  CAN-READ "*"
  CAN-WRITE "*"
