  TABLE-TRIGGER "CREATE" NO-OVERRIDE PROCEDURE "trg/gsteltrigc.p"
  TABLE-TRIGGER "DELETE" NO-OVERRIDE PROCEDURE "trg/gsteltrigd.p"
