  LABEL "Action Menu Label"
  VALMSG "s_label"
  HELP "Action menu label"

ADD FIELD "action_label" OF "ryc_action" AS CHAR
  DESCRIPTION "This is a short label for the button that would appear on the button as the button label in the event that the button has no image. It should contain ampersand (&) to act as the shortcut character.
