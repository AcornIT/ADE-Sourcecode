  INITIAL "0"
  LABEL "Container SmartObject Obj"
  VALMSG "o_obj"
  HELP "Container SmartObject obj"
  DECIMALS 6

ADD FIELD "smartobject_obj" OF "ryc_ui_event" AS DECIMAL
  FORMAT ">>>>>>>>>>>>>>>>>9.999999"
  INITIAL "0"
  LABEL "SmartObject Obj"
  VALMSG "o_obj"
  HELP "SmartObject obj"
  DECIMALS 6

