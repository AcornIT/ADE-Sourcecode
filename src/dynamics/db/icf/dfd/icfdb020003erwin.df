  DESCRIPTION "If set to YES, then this range will not be checked, and full access will be granted."
  FORMAT "YES/NO"
  INITIAL "NO"
  LABEL "Disabled"
  VIEW-AS "VIEW-AS TOGGLE-BOX"
  VALMSG "l_noyes"
  HELP "Disabled"

ADD FIELD "system_owned" OF "gsm_range" AS LOGICAL
