  DESCRIPTION "Detailed description of item"
  FORMAT "X(500)"
  LABEL "Item Narration"
  VIEW-AS "VIEW-AS EDITOR SIZE 70 BY 8 SCROLLBAR-VERTICAL MAX-CHARS 500"
  VALMSG "s_string_500"
  HELP "Item narration"

