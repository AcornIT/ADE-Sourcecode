  INITIAL "0"
  LABEL "Range Obj"
  VALMSG "o_obj"
  HELP "Range obj"
  DECIMALS 6

ADD FIELD "range_code" OF "gsm_range" AS CHAR
  DESCRIPTION "The unique code allocated to this range."
  FORMAT "X(10)"
  LABEL "Range Code"
  VALMSG "s_code"
  HELP "Range code"

ADD FIELD "range_description" OF "gsm_range" AS CHAR
  DESCRIPTION "A full description of the purpose of the range."
  FORMAT "X(35)"
  LABEL "Range Description"
  VALMSG "s_description"
  HELP "Range description"

