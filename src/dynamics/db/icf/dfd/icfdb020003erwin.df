  DESCRIPTION "This field indicates which user created this version of the data if known."
  FORMAT "X(20)"
  LABEL "Version User"
  VALMSG "s_string_20"
  HELP "Version user"

ADD FIELD "deletion_flag" OF "gst_record_version" AS LOGICAL
  DESCRIPTION "If set to yes, this field indicates that the last action against the record resulted in a deletion."
  FORMAT "YES/NO"
  INITIAL "NO"
  LABEL "Deletion Flag"
  VIEW-AS "VIEW-AS TOGGLE-BOX"
  VALMSG "l_noyes"
  HELP "Deletion flag"

